package hellojava;

import java.util.Scanner;

/**
 *
 * @author Admin
 */
public class NewClass2 {

    public static void main(String[] args) {
        System.out.println("Bai toan tinh giai thua");
        int n,s=1;
        Scanner input = new Scanner(System.in);
        n=input.nextInt();
        for(int i=1; i<=n; i++){
            s=s*i;
        }
        System.out.printf("%d giai thua = %d ",n,s);
    }
}
