package hellojava;

import java.util.Scanner;

/**
 *
 * @author Admin
 */
public class Baitapnop240720 {

    public static void main(String[] args) {
        Scanner input = new Scanner(System.in);
        int[] a = new int[5];
        for (int i = 0; i < 5; i++) {
            System.out.printf("A[%d]= ", i);
            a[i] = input.nextInt();
        }
        int n;
        System.out.println("Nhap vao so can kiem tra: ");
        n = input.nextInt();
        for (int i = 0; i < 5; i++) {
            if (a[i] == n) {
                System.out.printf("Co so %d trong mang o vi tri %d", a[i], i);
            }
        }
    }
}
